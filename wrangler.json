{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "ref-tool-frontend",
	"compatibility_date": "2025-10-08",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"main": "./dist/_worker.js/index.js",
	"assets": {
		"directory": "./dist",
		"binding": "ASSETS"
	},
	"observability": {
		"enabled": true
	},
	"services": [{ "binding": "API", "service": "ref-tool-worker" }],
	"upload_source_maps": true
}